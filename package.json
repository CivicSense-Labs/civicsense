{
  "name": "civicsense",
  "version": "1.0.0",
  "description": "AI-powered 311 & Property Issue Reporter with multi-agent workflow",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "node src/simple-server.js",
    "dev:ts": "tsc && node dist/server.js",
    "dev:watch": "nodemon --watch src --ext ts --exec \"npm run dev:ts\"",
    "start": "node src/simple-server.js",
    "build": "echo 'Using JavaScript version - no build needed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "func:serve": "supabase functions serve --env-file .env.local",
    "demo:seed": "node scripts/seed-demo.js",
    "prepare": "husky install || echo 'Husky not installed, skipping git hooks setup'"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "@langchain/community": "^0.0.43",
    "@langchain/core": "^0.1.48",
    "@langchain/openai": "^0.0.25",
    "@supabase/supabase-js": "^2.38.4",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "openai": "^4.28.4",
    "twilio": "^4.19.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "ai",
    "agents",
    "311",
    "municipal",
    "twilio",
    "supabase",
    "langchain"
  ],
  "author": "CivicSense Team",
  "license": "MIT",
  "engines": {
    "node": "22.x"
  }
}
